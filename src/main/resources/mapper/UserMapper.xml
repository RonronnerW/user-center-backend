<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wang.usercenter.mapper.UserMapper">

    <resultMap id="BaseResultMap" type="com.wang.usercenter.domain.User">
