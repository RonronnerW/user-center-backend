            <result property="tags" column="tags" jdbcType="VARCHAR"/>
            <result property="profile" column="profile" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,username,user_count,
        avatar_url,gender,user_pwd,
        email,user_status,phone,
        create_time,update_time,is_delete,planet_code,
        role,tags,profile
